{
  "name": "CIFAR10 Project",
  "dataset": {
    "name": "CIFAR10",
    "paths": ["/home/ben/Documents/img_library/streets",
             "/home/ben/Documents/img_library/street_view"],
    "save": true,
    "load": true,
    "batch_size": 2800,
    "shuffle": true,
    "split": 0.8
  },
  "run": {
    "num_epochs": 12
  },
  "models": {
    "name": "Resnet18",
    "task": "classification",
    "num_classes": 10,
    "pretrained": true
  },
  "loss": {
    "name": "CrossEntropyLoss"
  },
  "optim": {
    "name": "Adam",
    "lr": 0.005,
    "lr_min": 0.00025,
    "scheduler": true,
    "scheduler_type": "linear",
    "multiplier": 0.98
  },
  "server": {
    "host": "0.0.0.0",
    "port": 5000,
    "active": true
  }
}